void main() {
    int i = 100;
    float f = 33.333;
    sout("the current value of i is {i} and f is {f}");

    i += 50;
    f += 6.667;
    sout("the current value of i is {i} and f is {f}");

    i -= 50;
    f -= 10.0;
    sout("the current value of i is {i} and f is {f}");

    i *= 2;
    f *= 2.5;
    sout("the current value of i is {i} and f is {f}");

    i /= 2;
    f /= 2.5;
    sout("the current value of i is {i} and f is {f}");
}